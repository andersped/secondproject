<%= form_for([@user, @categorybookmark], html:{class:"form-horizontal", role:"form"}) do |f| %>
<%=render partial: 'category_bookmarks/errors', locals:{instance:@categorybookmark} %>

	<div class="form-group">
		<%= f.label :name, class:"col-md-2" %>
		<div class="col-sm-7">
			<%=f.text_field :name, class:"form-control" %>
		</div>
	</div>

	<div class="form-group">
    <%= f.collection_check_boxes(:bookmark_ids, @bookmarks, :id, :name) do |b| %>
        <span>
            <%= b.check_box %>
            <%= b.label %>
        </span>
    <% end %>
  </div>


	<div class="actions col-sm-offset-2">
		<%= f.submit class:"btn btn-primary" %>
		<%= link_to 'Cancel', user_category_bookmarks_path(current_user), class:"btn btn-default" %>
	</div>

	<% end %>